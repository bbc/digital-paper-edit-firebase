{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve:dev": "firebase serve -P dev --only functions",
    "serve:prod": "firebase serve -P prod --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy:dev": "firebase deploy -P dev --only functions",
    "deploy:prod": "firebase deploy -P prod --only functions",
    "logs": "firebase functions:log",
    "test": "mocha --reporter spec"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/speech": "^4.1.3",
    "@google-cloud/tasks": "^2.1.1",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.11.0",
    "fluent-ffmpeg": "^2.1.2",
    "gcp-to-dpe": "^2.0.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "fetch-mock": "^9.3.1",
    "firebase-functions-test": "^0.1.7",
    "mocha": "^7.1.1"
  },
  "private": true
}
